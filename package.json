{
  "name": "react-express-boilerplate",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start-client": "node clientApp/tools/start",
    "build-client": "tsc --noEmit --project clientApp/tsconfig.json && node clientApp/tools/build",
    "build-server": "rimraf build && tsc --project server/tsconfig.json && run-s cpy-server-files",
    "build-all": "run-s build-server build-client",
    "eslint-client": "eslint 'clientApp/src/**/*.{js,ts,tsx}'",
    "graphql-gen": "graphql-codegen --config codegen.yml",
    "cpy-server-files": "run-s cpy-server-files:statics cpy-server-files:configs",
    "cpy-server-files:statics": "copyfiles -u 3 server/application/assets/**/* build/server/application/assets",
    "cpy-server-files:configs": "copyfiles -u 2 common/config/**/*.yml build/common/config"
  },
  "_moduleAliases": {
    "@application": "build/server/application",
    "@application.core": "build/server/application.core",
    "@domain": "build/server/domain",
    "@domain.core": "build/server/domain.core",
    "@infraestructure": "build/server/infraestructure",
    "@infraestructure.core": "build/server/infraestructure.core",
    "@common": "build/common"
  },
  "author": "",
  "license": "ISC",
